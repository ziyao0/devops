{
  "properties": [
    {
      "name": "com.ziyao.jenkins.base-uri",
      "type": "com.ziyao.devops.jenkins.properties.JenkinsProperties",
      "description": "Jenkins Access to the address",
      "defaultValue": "http://localhost:8080/jenkins"
    },
    {
      "name": "com.ziyao.jenkins.access-key",
      "type": "com.ziyao.devops.jenkins.properties.JenkinsProperties",
      "description": "username to use when connecting",
      "defaultValue": "jenkins"
    },
    {
      "name": "com.ziyao.jenkins.secret-key",
      "type": "com.ziyao.devops.jenkins.properties.JenkinsProperties",
      "description": "password (not recommended) or token (recommended)",
      "defaultValue": "jenkins"
    }
  ]
}